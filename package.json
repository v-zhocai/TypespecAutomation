{
  "name": "TypespecUIAutomation",
  "displayName": "TypespecUIAutomation",
  "version": "0.0.1",
  "description": "Typespec UI automation project based on playwright",
  "license": "MIT",
  "scripts": {
    "test": "vitest --root src",
    "test:create_win": "vitest --root ./src/createTypespec",
    "test:emit_win": "vitest --root ./src/emitTypespec",
    "test:import_win": "vitest --root ./src/importTypespec",
    "test:preview_win": "vitest --root ./src/previewTypespec",
    "test:all_win": "npm run test:create_win && npm run test:emit_win && npm run test:import_win && npm run test:preview_win",

    "test:create_linux": "vitest --root ./src/createTypespec",
    "test:emit_linux": "vitest --root ./src/emitTypespec",
    "test:import_linux": "vitest --root ./src/importTypespec",
    "test:preview_linux": "vitest --root ./src/previewTypespec",
    "test:all_linux": "npm run test:create_linux && npm run test:emit_linux && npm run test:import_linux && npm run test:preview_linux"
  },
  "dependencies": {
    "@nut-tree-fork/nut-js": "^4.2.6",
    "@types/node": "^20.12.8",
    "@types/screenshot-desktop": "^1.12.3",
    "@typespec/compiler": "^1.0.0-rc.1",
    "@typespec/http": "^1.0.0-rc.1",
    "@typespec/http-client-csharp": "^1.0.0-alpha.20250424.2",
    "@typespec/http-client-java": "^0.3.2",
    "@typespec/http-client-js": "^0.4.0",
    "@typespec/http-client-python": "^0.11.2",
    "@typespec/http-server-csharp": "^0.58.0-alpha.14",
    "@typespec/http-server-js": "^0.58.0-alpha.13",
    "@typespec/openapi": "^1.0.0-rc.1",
    "@typespec/openapi3": "^1.0.0-rc.1",
    "@vscode/test-electron": "^2.3.9",
    "moment": "^2.30.1",
    "playwright": "^1.51.1",
    "screenshot-desktop": "^1.15.1",
    "typescript": "^5.6.2",
    "vitest": "^2.1.2"
  }
}
